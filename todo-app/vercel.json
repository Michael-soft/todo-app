{
  "version": 2,
  "builds": [
    {
      "src": "todo-app/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "todo-app/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/todo-app/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/todo-app/server.js"
    }
  ]
}